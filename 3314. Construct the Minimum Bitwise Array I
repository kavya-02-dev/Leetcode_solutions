class Solution {
    public int[] minBitwiseArray(List<Integer> nums) {
        int ans[]=new int[nums.size()];
        
        
        for(int i=0;i<nums.size();i++){
            int k=0;
            boolean flag=false;
            while((0<=k && k<=nums.get(i)) && flag==false){
                if ((k | k + 1) == nums.get(i)){
                    ans[i]=k;
                    flag=true;
                }
                else{
                    k++;
                }

            }
            if (flag==false){
                ans[i]=-1;
            }


        }
        return ans;
    }
}
