class Solution {
    public int[] searchRange(int[] nums, int target) {
        int i=0;
        int j=nums.length-1;
        int res[]=new int[2];
        boolean flag=false;
        boolean flag2=false;
        while(i<=j){
            
            if((nums[i]==target) && (flag==false)){
                 flag=true;
                 res[0]=i;
                 

            }  
            if((nums[j]==target) && (flag2==false)){  
                 flag2=true;  
                 res[1]=j;  
                
            }  
            if(flag2==false){
                 j--;
            }
            if( flag==false){
                 i++;
            }
            if(flag && flag2) break;
          
          
        }  
        if (flag==false) res[0]=-1;  
        if (flag2==false) res[1]=-1;  
        return res;  
      
    }

}
